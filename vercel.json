{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    },
    {
      "source": "/service.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript",
          "comment": "https://bugs.chromium.org/p/chromium/issues/detail?id=1292379"
        }
      ]
    }
  ]
}
